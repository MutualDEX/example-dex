{
  "name": "example-dex-remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "generate:css": "unocss './app/**/*.{js,ts,jsx,tsx}' -o ./app/styles/uno.css",
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev --manual",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "format": "prettier --write . && yarn eslint --fix .",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@orderly.network/hooks": "1.1.4",
    "@orderly.network/types": "^0.2.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^2.0.3",
    "@remix-run/css-bundle": "^2.8.0",
    "@remix-run/node": "^2.8.0",
    "@remix-run/react": "^2.8.0",
    "@remix-run/serve": "^2.8.0",
    "@web3-onboard/injected-wallets": "^2.10.11",
    "@web3-onboard/react": "^2.8.13",
    "@web3-onboard/walletconnect": "^2.5.3",
    "axios": "^1.6.7",
    "ethers": "^6.11.1",
    "isbot": "^5.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.8.0",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@unocss/cli": "^0.58.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "unocss": "^0.58.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
